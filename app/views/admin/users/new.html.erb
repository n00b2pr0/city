<div class="page-header">
  <h1>Create User</h1>
</div>
<%= form_for [:admin,  @user] do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <div class="form-actions">
    <%= f.submit "Create User", :class => "btn btn-success" %>
  </div>
<% end %>
